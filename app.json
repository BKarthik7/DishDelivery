{
  "expo": {
    "name": "DishDelivery",
    "slug": "DishDelivery",
    "version": "1.0.0",

    "web": {
      "favicon": "./assets/icons/favicon.ico"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [],

    "orientation": "portrait",
    "icon": "./assets/icons/android-chrome-512x512.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/icons/android-chrome-512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.company.dishdelivery"
    }
  }
}
